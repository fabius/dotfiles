{
  "coc.preferences.extensionUpdateCheck": "daily",
  "toml.formatter.compactArrays": true,
  "toml.formatter.indentTables": true,
  "toml.formatter.arrayAutoExpand": true,
  "toml.formatter.arrayAutoCollapse": true,
  "toml.formatter.arrayTrailingComma": true,
  "toml.formatter.compactInlineTables": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "go",
    "html",
    "javascript",
    "json",
    "markdown",
    "python",
    "rust",
    "sql",
    "svelte",
    "toml",
    "vue",
    "xml",
    "yaml"
  ],
  "explorer.file.showHiddenFiles": true,
  "rust-analyzer.server.path": "/opt/homebrew/bin/rust-analyzer"
}
